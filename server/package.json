{
  "name": "task-server",
  "version": "1.0.0",
  "description": "Server app for test assignment",
  "main": "index.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "generate": "graphql-codegen --config codegen.ts",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "build:sources": "tsc",
    "start": "node ./dist/index.js",
    "start:debug": "NODE_OPTIONS=--inspect netlify dev",
    "start:dev": "ENV=service nodemon src/index.ts",
    "start:netlify-dev": "netlify dev",
    "prisma:dev-db-push": "npx prisma db push",
    "prisma:migrate-dev": "npx prisma migrate dev",
    "prisma:migrate": "npx prisma migrate dev --skip-seed",
    "prisma:seed": "npx prisma db seed",
    "postinstall": "npx prisma generate",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Zakhar Gulchak <gulchak.zahar@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-document-nodes": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/node": "^18.17.15",
    "csv-parse": "^5.4.0",
    "netlify-cli": "^16.3.2",
    "nodemon": "^3.0.1",
    "prisma": "^5.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "@prisma/client": "^5.3.0",
    "apollo-server-lambda": "^3.12.1",
    "graphql": "^16.8.0",
    "undici": "^5.24.0"
  }
}
